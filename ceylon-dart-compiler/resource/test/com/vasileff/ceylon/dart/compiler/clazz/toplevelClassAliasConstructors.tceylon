shared class C {
    shared Integer i;
    shared new (Integer a = 10) {
        this.i = a;
    }
    shared new create(Integer a = 10, Integer b = 20) {
        this.i = a + b;
    }
}

shared class CAlias(Integer x) => C(x);
//shared class CCreateAlias(Integer x, Integer y) => C.create(x, y);

shared void run() {
    // positional args
    assert (CAlias.create(10, 20).i == 30);
    //assert (CCreateAlias.create(10, 20).i == 30);

    // positional args using defaults
    assert (CAlias.create().i == 30);
    //assert (CCreateAlias.create().i == 30);

    // named args
    assert (CAlias.create {a = 10; b = 20;}.i == 30);
    //assert (CCreateAlias.create {a = 10; b = 20;}.i == 30);

    // named args using defaults
    assert (CAlias.create {a = 10; b = 20;}.i == 30);
    //assert (CCreateAlias.create {a = 10; b = 20;}.i == 30);
}