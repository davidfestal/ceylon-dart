class C() {
    String(String) v1;
    v1(String s) => s;
    assert (v1("hello").size == 5);

    String(String) v2;
    v2 = (String s) => s;
    assert (v2("hello").size == 5);

    String(String) v3;
    v3 => (String s) => s;
    assert (v3("hello").size == 5);

    String f1(String s);
    f1(String s) => s;
    assert (f1("hello").size == 5);

    String f2(String s);
    f2 = (String s) => s;
    assert (f2("hello").size == 5);

    print("done!");
}

